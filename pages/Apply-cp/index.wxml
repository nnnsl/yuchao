<!--pages/Apply_cp/index.wxml-->
<view class="Apply_cp">
  <view class="Apply_main">


    <view class="name">
      <text>真实姓名</text>
      <input placeholder="请输入您的姓名"></input>
    </view>

    <view class="phone">
      <text>联系方式</text>
      <input placeholder="请输入联系方式"></input>
    </view>

    <view class="papers">
      <text>是否持有裁判证</text>
      <van-switch checked="{{ checked }}" bind:change="onChange" size="38rpx" active-color="#FFC000" inactive-color="#ccc" />
    </view>

    <view class="submit-btn">
<view class="text">提交申请</view>

<image src="/images/my/ty.png"></image>
</view>



  </view>

</view>